  {
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["declarativeContent", "storage", "tabs", "activeTab"],
    "background": {
      "scripts": ["background.js", "search.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["*://www.ebay.com/sch/i.html*"],
        "js": ["search.js"]
      }
    ],
    "page_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }
